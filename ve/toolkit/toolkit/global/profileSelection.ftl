profile-selection-window =
    .title = { -brand-short-name } - Nangani Mbonwasia ya Mushumisi

profile-selection-button-accept =
    .label = Thomani { -brand-short-name }

profile-selection-button-cancel =
    .label = Ibvani

profile-selection-new-button =
    .label = Vhumbani Mbonwasia...
    .accesskey = V

profile-selection-rename-button =
    .label = Bvisani Mbonwasia...
    .accesskey = B

